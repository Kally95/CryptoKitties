<% if policy(@kitty).edit? %>
  <%= link_to 'Edit the KITTY', edit_kitty_path(@kitty), :class=>"btn btn-primary btn-gradient button-landing sticky-bottom" %> |
<% end %
<%= link_to 'Back to all KITTY page', kitties_path, :class=>"btn btn-primary btn-gradient button-landing sticky-bottom" %>


<%= link_to "Back to home page", root_path, :class=>"btn btn-danger btn-gradient button-landing sticky-bottom"  %>

<% if policy(@kitty).destroy? %>
  <%= link_to 'Destroy', @kitty, method: :delete, class: "btn btn-danger btn-gradient button-landing sticky-bottom", data: { confirm: 'Are you sure you want to delete this lovely kitty ???' } %>
<% end %>

<div class="booking-section">
  <h2>BOOK THIS KITTY</h2>

  <%= link_to "BOOK THE KITTY", new_kitty_booking_path(@kitty), :class=>"btn btn-danger btn-gradient button-landing sticky-bottom"  %>
   <%= link_to "SEE ALL KITTIES YOU BOOKED", kitty_bookings_path(@kitty), :class=>"teach-btn btn button-landing sticky-bottom" %>

</div>




