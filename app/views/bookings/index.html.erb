<h1 style="text-align: center; color: #ef51d2; margin-top: 20px;">ALLL KITTIES BOOKING</h1>
<div class="container">
  <div class="row" style="width:100%">
    <% @bookings.each do |booking| %>
      <div class="col-sm-3">
        <h2><%= link_to booking.kitty.public_key, booking_path(booking), :class=>"teach-btn btn button-landing sticky-bottom" %></h2>
        <h2><%= booking.user.nickname %></h2>
        <h2><%= booking.start_date %></h2>
        <h2><%= booking.end_date %></h2>
      </div>
    <% end %>
  </div>
</div>
<p><%= link_to "Back to ALL KITTIES", kitties_path, :class=>"teach-btn btn button-landing sticky-bottom" %></p>
