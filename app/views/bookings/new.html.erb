<h1 style="text-align: center;"> BOOK A NEW KITTY </h1>
<div class="container">
  <div class="row">
    <div class="col-sm-4 offset-sm-4">
      <%= simple_form_for [@kitty, @booking] do |f| %>
        <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.input :end_date, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.button :submit, "GoGoGoGoGo", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
